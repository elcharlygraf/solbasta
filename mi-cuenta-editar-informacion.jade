									b  gana solsazos.
						li: a(href="#", title="#") Compra solsazos
						li: a(href="#", title="#") Ayuda

				section#editar-informacion
					h1
						span Editar informacion
					.desarrollo
						form
							p
								img(src="imagenes/avatar.png" alt="Avatar")#avatar
								span verocn1093
								span Veronica Campos
								span verockn1093@hotmail.com
								i
									span.icon-lapiz2
							p
								input(name="clave-actual", autocomplete="off", placeholder="Contraseña", type="password", value="Una contraseña")
								span.icon-candado1
							p
								input(name="nueva-clave1", autocomplete="off", placeholder="Nueva contraseña", type="password", value="")
								span.icon-candado2
							p
								input(name="nueva-clave2", autocomplete="off", placeholder="Repetir nueva contraseña", type="password", value="")
								span.icon-candado2
							p
								input(name="nombre", placeholder="Nombre", type="text", value="Veronica Campos Nieves")
								span.icon-usuario
							p
								input(name="email", placeholder="Correo electrónico", type="email", value="verocn1093@hotmail.com")
								span.icon-email
							p
								input(name="usuario", placeholder="Nombre de usuario", type="text", value="verocn1093")
								span.icon-usuario2
							p
								input(name="telefono", placeholder="Numero de telefono", type="text", value="71548245")
								span.icon-carnet
							p
								select(name="sexo")
