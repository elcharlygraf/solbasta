								span.icon-flecha
							p
								input(name="celular", placeholder="Número celular", type="text", value="")
								span.icon-telefono
							p
								input(name="nacimiento", placeholder="Nacimiento", type="text", value="10/08/93", data-language="es").datepicker-here
								span.icon-torta2
							p
								input(name="submit", value="Guardar cambios", type="submit")

						p#iconos.accesos-directos
							button: span.icon-archivos2
							button: span.icon-play
							button: span.icon-camara
							button: span.icon-reloj
							button: span.icon-regalo
					
					#secciones
						section.suscripcion
							h2 Mi suscripción
							.contenedor
								.articulos
									article.accion
										h3 Si no deseas recibir más promociones en tu correo electrónico, por favor, da click aquí:
										button Cancelar suscripción
						section.videos
							h2 Mis Videos
							.contenedor
								.articulos
									-for(var v = 0; v <= 4; v++)
										article
											.imagen
												img(src="imagenes/hombre.png", alt="#")
											.texto
												h3 
													span Tesminio de
													|  Monopoly #{v + 1}
												a(href="#", title="#") Ver video
									article.accion
										.imagen
												span.icon-play
										.texto
											h3 Agrega un video
												span y gana solsazos
											a(href="#", title="#") Sube un video
							button: span.icon-flecha-izquierda
							button: span.icon-flecha-derecha
						section.fotos
							h2 Mis Fotos
							.contenedor
								.articulos
									-for(var v = 0; v <= 4; v++)
										article
											.imagen
												img(src="imagenes/hombre.png", alt="#")
											.texto
												h3 
													span Foto de
													|  Monopoly #{v + 1}
												a(href="#", title="#") Ver foto
									article.accion
										.imagen
												span.icon-camara
										.texto
											h3 Agrega una foto
												span y gana solsazos
											a(href="#", title="#") Sube una foto
							button: span.icon-flecha-izquierda
							button: span.icon-flecha-derecha
						section.recordatorios
							h2 Mis recordatorios
							.contenedor
								.articulos
									-for(var r = 0; r <= 7; r++)
										article
											button 
												span.icon-equis
												|	Eliminar
											.imagen
												img(src="imagenes/proximas/proximas/#{r + 1}.png", alt="#")
											.texto
												p
													time(datetime="2015-05-17") 
														b Martes 1#{r + 1}
														| /05/16
													| 1#{r + 1}:00:00 PM
												h3 Mochila Adidas
												h4 precio de mercado
													b  s/.#{r + 1}00
