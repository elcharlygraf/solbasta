										.imagen
											span.icon-facebook
										.texto
											h3 
												span Ganaste este codigo
												| Para canejar solsazos
											b FRT4582T
									article
										.imagen
											span.icon-twitter
										.texto
											h3 
												span Ganaste este codigo
												| Para canejar solsazos
											b FRT4582T
									article
										.imagen
											span.icon-instagram
										.texto
											h3 
												span Ganaste este codigo
												| Para canejar solsazos
											b FRT4582T	
									article
										.imagen
											span.icon-facebook
										.texto
											h3 
												span Ganaste este codigo
												| Para canejar solsazos
											b FRT4582T	
